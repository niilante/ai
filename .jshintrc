{
    "node": true,
    "browser": true,
    "esnext": true,
    "bitwise": true,
    "camelcase": true,
    "curly": false,
    "eqeqeq": true,
    "immed": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "regexp": true,
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "smarttabs": true,
    "maxlen": 125,
    "predef": [
        "angular",
        "_",
        "$MODULES",
        "$$DB",
        "$$PKG",
        "$$SETTINGS",
        "$$SERVICES",
        "$$LOG",
        "$$DEBUG",
        "$$CONTROLLERS",
        "$$UTILS",
        "$$PATHS"
    ]
}
